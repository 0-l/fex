{
  "keybindings": {
    "+": {
        "description": "create a tab",
        "function": "tabs.create()"
    },
    "=": {
        "function": "tabs.create()",
        "description": "create a tab"
    },
    "-": {
        "function": "tabs.remove()",
        "description": "delete the current tab"
    },
    "x": {
        "function": "tabs.remove()",
        "description": "delete the current tab"
    },
    "#": {
        "function": "new Image().viewImages",
        "description": "toggle images"
    },
    "<": {
        "function": "goBack()",
        "description": "go to the previous directory"
    },
    ">": {
        "function": "goForward()",
        "description": "go forward one directory"
    },
    "|": {
        "function": "menu.toggle()",
        "description": "toggle the menu"
    },
    "?": {
        "function": "this.show()",
        "description": "toggle this page"
    },
    "alt": {
      "37": "goBack()",
      "39": "goForward()",
      "38": "this.upDir()"
    },
    "vim": {
      "ArrowDown": "25",
      "ArrowUp": "-25",
      "j": "10",
      "k": "-10",
      "s": "25",
      "w": "-25",
      "d": "50",
      "e": "-50",
      "G": "this.height",
      "g": "-(this.height)",
      "v": "new Tabs().split()",
      "S": "goBack()",
      "D": "goForward()"
    }
  }
}
